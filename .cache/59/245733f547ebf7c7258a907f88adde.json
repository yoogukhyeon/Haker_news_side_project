ws-detail-view.ts","name":null,"original":{"line":90,"column":2},"generated":{"line":71,"column":0}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":6,"column":0},"generated":{"line":71,"column":1}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":6,"column":44},"generated":{"line":71,"column":2}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":6,"column":44},"generated":{"line":71,"column":8}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":6,"column":44},"generated":{"line":71,"column":9}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":6,"column":48},"generated":{"line":71,"column":16}},{"source":"src/page/news-detail-view.ts","name":null,"original":{"line":6,"column":48},"generated":{"line":71,"column":17}}],"sources":{"src/page/news-detail-view.ts":"import View from \"../core/view\";\r\nimport {NewsDetailApi} from \"../core/api\";\r\nimport { NewsStore, NewsComment, NewsDetail } from \"../types\";\r\nimport {CONTENT_URL} from '../config';\r\n\r\nexport default class NewsDetailView extends View{\r\n\r\n    private store : NewsStore;\r\n\r\n    constructor(containerId: string, store: NewsStore){\r\n    \r\n      const templete =  `\r\n        <div class=\"bg-gray-600 min-h-screen pb-8\">\r\n          <div class=\"bg-white text-xl\">\r\n            <div class=\"mx-auto px-4\">\r\n              <div class=\"flex justify-between items-center py-6\">\r\n                <div class=\"flex justify-start\">\r\n                  <h1 class=\"font-extrabold\">Hacker News</h1>\r\n                </div>\r\n                <div class=\"items-center justify-end\">\r\n                  <a href=\"#/page/{{__currentPage__}}\" class=\"text-gray-500\">\r\n                    <i class=\"fa fa-times\"></i>\r\n                  </a>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n    \r\n          <div class=\"h-full border rounded-xl bg-white m-6 p-4 \">\r\n            <h2>{{__title__}}</h2>\r\n            <div class=\"text-gray-400 h-20\">\r\n              {{__content__}}\r\n            </div>\r\n              {{__comments__}}\r\n          </div>\r\n        </div>\r\n      `;  \r\n      \r\n      super(containerId, templete);\r\n\r\n      this.store = store;\r\n  \r\n    }\r\n  \r\n    render(){\r\n      const id = location.hash.substr(7);\r\n      const api = new NewsDetailApi(CONTENT_URL.replace('@id', id))\r\n        \r\n      api.getData((data: NewsDetail) => {\r\n    \r\n        const newsContent = data;\r\n\r\n        this.store.makeRead(Number(id));\r\n        this.setTemplateData('comments', this.makeComment(newsContent.comments));\r\n        this.setTemplateData('currentPage', String(this.store.currentPage));\r\n        this.setTemplateData('title', newsContent.title);\r\n        this.setTemplateData('content', newsContent.content);\r\n\r\n        this.updateView()\r\n\r\n      })\r\n  \r\n      \r\n    \r\n    }\r\n  \r\n  \r\n    makeComment(comments: NewsComment[]): string {\r\n   \r\n      for(let i = 0; i < comments.length; i++){\r\n        const comment: NewsComment = comments[i];\r\n    \r\n        this.addHtml(`\r\n          <div style=\"padding-left: ${comment.level * 40}px;\" class=\"mt-4\">\r\n            <div class=\"text-gray-400\">\r\n              <i class=\"fa fa-sort-up mr-2\"></i>\r\n              <strong>${comment.user}</strong> ${comment.time_ago}\r\n            </div>\r\n            <p class=\"text-gray-700\">${comment.content}</p>\r\n          </div> \r\n        `);\r\n    \r\n        if(comment.comments.length > 0){\r\n          this.addHtml(this.makeComment(comment.comments));\r\n        }\r\n      };\r\n    \r\n      return this.getHtml();\r\n    }\r\n  }"},"lineCount":null}},"error":null,"hash":"4c018f4aa83ad7cd8c522fd499e48d8f","cacheData":{"env":{}}}